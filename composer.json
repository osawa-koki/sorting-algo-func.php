{
  "name": "osawa-koki/sorting-algo-func-php",
  "description": "📊📊📊 PHPで整列アルゴリズムを実装してみる！",
  "license": "Apache-2.0",
  "scripts": {
    "start": [
      "php ./src/main.php"
    ],
    "lint": [
      "php-cs-fixer check ./src/",
      "php-cs-fixer check ./tests/"
    ],
    "lint:fix": [
      "php-cs-fixer fix ./src/",
      "php-cs-fixer fix ./tests/"
    ],
    "test": [
      "phpunit"
    ]
  },
  "autoload": {
    "psr-4": {
      "App\\": "./src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^11.1",
    "friendsofphp/php-cs-fixer": "^3.54"
  }
}
